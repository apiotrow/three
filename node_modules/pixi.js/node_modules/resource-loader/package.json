    "pretest": "npm run build",
    "test": "npm run test-dev -- --single-run",
    "test-dev": "karma start test/karma.conf.js",
    "docs": "jsdoc -c jsdoc.conf.json -R README.md",
    "prepublish": "npm run build && npm run minify"
